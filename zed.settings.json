{
  "collaboration_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "context_servers": {
    "notion-tasks": {
      "source": "custom",
      "enabled": true,
      "command": "npx",
      "args": ["-y", "@notionhq/notion-mcp-server"],
      "env": {
        "OPENAPI_MCP_HEADERS": "{\"Authorization\": \"Bearer ntn_**\", \"Notion-Version\": \"2022-06-28\" }"
      }
    },
    "mcp-server-context7": {
      "source": "extension",
      "settings": {
        "default_minimum_tokens": "10000"
      }
    }
  },
  "vim_mode": true,
  "chat_panel": {
    "dock": "left"
  },
  "git_panel": {
    "dock": "left"
  },
  "agent": {
    "inline_assistant_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-latest"
    },
    "play_sound_when_agent_done": true,
    "profiles": {
      "write": {
        "name": "Write",
        "tools": {
          "copy_path": true,
          "create_directory": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "list_directory": true,
          "project_notifications": false,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "terminal": true,
          "thinking": true,
          "web_search": true
        },
        "enable_all_context_servers": true,
        "context_servers": {
          "notion-tasks": {
            "tools": {
              "API-update-a-database": false,
              "API-update-a-block": false,
              "API-retrieve-a-page-property": false,
              "API-retrieve-a-page": false,
              "API-retrieve-a-database": false,
              "API-retrieve-a-comment": false,
              "API-retrieve-a-block": false,
              "API-post-search": false,
              "API-post-page": false,
              "API-post-database-query": false,
              "API-patch-page": false,
              "API-patch-block-children": false,
              "API-get-users": false,
              "API-get-user": false,
              "API-get-self": false,
              "API-get-block-children": false,
              "API-delete-a-block": false,
              "API-create-a-database": false,
              "API-create-a-comment": false
            }
          }
        }
      },
      "architech": {
        "name": "Architech",
        "tools": {
          "terminal": true,
          "batch_tool": true,
          "code_symbols": true,
          "copy_path": true,
          "create_file": true,
          "delete_path": true,
          "diagnostics": true,
          "edit_file": true,
          "edit_files": false,
          "fetch": true,
          "list_directory": true,
          "move_path": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "symbol_info": true,
          "thinking": true
        },
        "enable_all_context_servers": true,
        "context_servers": {}
      },
      "ask": {
        "name": "Ask",
        "tools": {
          "web_search": true,
          "terminal": false,
          "diagnostics": true,
          "fetch": true,
          "list_directory": true,
          "now": true,
          "find_path": true,
          "read_file": true,
          "grep": true,
          "thinking": true,
          "batch_tool": true
        },
        "enable_all_context_servers": false,
        "context_servers": {
          "notion-tasks": {
            "tools": {
              "API-update-a-database": false,
              "API-post-page": false,
              "API-post-search": true,
              "API-retrieve-a-block": true,
              "API-retrieve-a-page-property": true,
              "API-retrieve-a-page": true,
              "API-retrieve-a-database": true,
              "API-retrieve-a-comment": true,
              "API-get-user": true,
              "API-get-users": false
            }
          },
          "mcp-server-context7": {
            "tools": {
              "resolve-library-id": true,
              "get-library-docs": true
            }
          },
          "context7": {
            "tools": {
              "resolve-library-id": true,
              "get-library-docs": true
            }
          },
          "mcp-server-sequential-thinking": {
            "tools": {
              "sequentialthinking": true
            }
          }
        }
      }
    },
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_model": {
      "provider": "anthropic",
      "model": "claude-opus-4-thinking-latest"
    },
    "dock": "left",
    "default_width": 450
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "project_panel": {
    "dock": "right",
    "auto_fold_dirs": false
  },
  "theme": "VSCode Dark Polished",
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "show_wrap_guides": true,
  "wrap_guides": [88, 120],
  "soft_wrap": "editor_width",
  "show_whitespaces": "all",
  "relative_line_numbers": true,
  "indent_guides": {
    "enabled": true,
    "coloring": "indent_aware",
    "background_coloring": "indent_aware"
  },
  "terminal": {
    "dock": "left",
    "font_family": "Hack",
    "font_size": 13,
    "line_height": {
      "custom": 1.2
    }
  },
  "auto_install_extensions": {
    "html": true,
    "ruff": true
  }
}
